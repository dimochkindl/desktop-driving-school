<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="300.0" stylesheets="@../styles/style.css" prefWidth="400.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.db.controllers.AddWindowController">
    <VBox fx:id="fieldsContainer" alignment="TOP_LEFT" prefHeight="300.0" prefWidth="400.0" spacing="10">
    </VBox>
    <HBox alignment="CENTER" prefHeight="26.0" prefWidth="378.0" layoutY="250.0">
        <children>
            <Button fx:id="submitButton" alignment="CENTER" mnemonicParsing="false" text="Submit" />
        </children>
    </HBox>
    <Button fx:id="exitButton" mnemonicParsing="false" onAction="#handleExitButton" text="Exit" layoutY="250.0" />
</AnchorPane>